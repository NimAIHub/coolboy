	local Tabs = {}
	local firstchild = true
	
	function Tabs:NewTab(tinfo)
		tinfo = tinfo or "TAB"
		local Tab = Instance.new("TextButton")
		local NewPage = Instance.new("ScrollingFrame")
		local SearchBarF = Instance.new("Frame")
		local SearchCorner = Instance.new("UICorner")
		local SearchBox = Instance.new("TextBox")
		local SearchLabel = Instance.new("ImageLabel")
		local PageList = Instance.new("UIListLayout")
		
		Tab.MouseButton1Click:Connect(function()
			for i,v in next, Archives:GetChildren() do
				v.Visible = false
			end
			NewPage.Visible = true
			
			for i,v in next, AllTabs:GetChildren() do   -- We get all the elements inside the frame
				if v:IsA("TextButton") then -- We can't animate UIListLayout, so we check if its a TextButton
					game.TweenService:Create(v, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
						TextColor3 = Color3.fromRGB(72, 72, 72) -- We animate other Tab Buttons and making the current one seem Checked
					}):Play()
				end
			end
			game.TweenService:Create(Tab, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
				TextColor3 = Color3.fromRGB(60, 161, 255) -- We animate other Tab Buttons and making the current one seem Checked
			}):Play()
		end)
		
		Tab.Name = "Tab"
		Tab.Parent = AllTabs
		Tab.BackgroundColor3 = Color3.fromRGB(74, 74, 74)
		Tab.BackgroundTransparency = 1.000
		Tab.Size = UDim2.new(0, 111, 0, 20)
		Tab.Font = Enum.Font.GothamBold
		Tab.Text = tinfo
		Tab.TextColor3 = Color3.fromRGB(255, 255, 255)
		Tab.TextSize = 13.000
		Tab.TextWrapped = true
		Tab.TextXAlignment = Enum.TextXAlignment.Left

		NewPage.Name = "NewPage"
		NewPage.Parent = Archives
		NewPage.Active = true
		NewPage.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		NewPage.BackgroundTransparency = 1.000
		NewPage.BorderSizePixel = 0
		NewPage.Position = UDim2.new(0.0173913036, 0, 0.0227272734, 0)
		NewPage.Size = UDim2.new(0, 345, 0, 252)
		NewPage.CanvasSize = UDim2.new(0, 0, 0, 0)
		NewPage.ScrollBarThickness = 0
		NewPage.Visible = false

		SearchBarF.Name = "SearchBarF"
		SearchBarF.Parent = NewPage
