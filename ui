		local Elements = {}
		function Elements:CreateButton(btninfo, callback)
			btninfo = btninfo or "THIS IS A BUTTON"
			callback = callback or function() end
			
			local cmdbtn = Instance.new("TextButton")
			local cmd = Instance.new("TextLabel")
			local cmdCorner = Instance.new("UICorner")
			
			cmdbtn.Name = "cmdbtn"..btninfo
			cmdbtn.Parent = NewPage
			cmdbtn.BackgroundColor3 = Color3.fromRGB(24, 24, 24)
			cmdbtn.Position = UDim2.new(0, 0, 0.112854116, 0)
			cmdbtn.Size = UDim2.new(0, 340, 0, 25)
			cmdbtn.AutoButtonColor = false
			cmdbtn.Font = Enum.Font.GothamBold
			cmdbtn.Text = ""
			cmdbtn.TextColor3 = Color3.fromRGB(0, 0, 0)
			cmdbtn.TextSize = 14.000
			cmdbtn.MouseButton1Down:Connect(function()
				game.TweenService:Create(cmdbtn, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
					BackgroundColor3 = Color3.fromRGB(43, 43, 43)
				}):Play()
			end)
			cmdbtn.MouseButton1Up:Connect(function()
				callback()
				game.TweenService:Create(cmdbtn, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
					BackgroundColor3 = Color3.fromRGB(24, 24, 24)
				}):Play()
			end)
			cmdbtn.MouseLeave:Connect(function()
				game.TweenService:Create(cmdbtn, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
					BackgroundColor3 = Color3.fromRGB(24, 24, 24)
				}):Play()
			end)
			

			cmd.Parent = cmdbtn
			cmd.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			cmd.BackgroundTransparency = 1.000
			cmd.Position = UDim2.new(0.0352941193, 0, 0.322580636, 0)
			cmd.Size = UDim2.new(0, 306, 0, 10)
			cmd.Font = Enum.Font.GothamMedium
			cmd.Text = btninfo
			cmd.TextColor3 = Color3.fromRGB(255, 255, 255)
			cmd.TextSize = 12.000
			cmd.TextXAlignment = Enum.TextXAlignment.Left

			cmdCorner.CornerRadius = UDim.new(0, 5)
			cmdCorner.Name = "cmdCorner"
			cmdCorner.Parent = cmdbtn
			end
			
			function Elements:CreateToggle(toginfo, callback)
				toginfo = toginfo or "THIS IS A TOGGLE"
				callback = callback or function() end
				
				local togged = false
				local cmdtog = Instance.new("TextButton")
				local togcmd = Instance.new("TextLabel")
				local togCorner = Instance.new("UICorner")
				local Toogle = Instance.new("Frame")
				local togCorner_2 = Instance.new("UICorner")
			   local togStroke = Instance.new("UIStroke")
			
			cmdtog.MouseButton1Down:Connect(function()
				game.TweenService:Create(cmdtog, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
					BackgroundColor3 = Color3.fromRGB(43, 43, 43)
				}):Play()
			end)
			cmdtog.MouseButton1Up:Connect(function()
				game.TweenService:Create(cmdtog, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
					BackgroundColor3 = Color3.fromRGB(24, 24, 24)
				}):Play()
			end)
			cmdtog.MouseLeave:Connect(function()
				game.TweenService:Create(cmdtog, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
					BackgroundColor3 = Color3.fromRGB(24, 24, 24)
				}):Play()
			end)
				
				cmdtog.Name = "cmdtog"..toginfo
				cmdtog.Parent = NewPage
				cmdtog.BackgroundColor3 = Color3.fromRGB(24, 24, 24)
				cmdtog.ClipsDescendants = true
				cmdtog.Position = UDim2.new(0, 0, 0.112854116, 0)
				cmdtog.Size = UDim2.new(0, 340, 0, 25)
				cmdtog.AutoButtonColor = false
				cmdtog.Font = Enum.Font.GothamBold
				cmdtog.Text = ""
				cmdtog.TextColor3 = Color3.fromRGB(0, 0, 0)
				cmdtog.TextSize = 14.000

				togcmd.Name = "togcmd"
				togcmd.Parent = cmdtog
				togcmd.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				togcmd.BackgroundTransparency = 1.000
				togcmd.Position = UDim2.new(0.0352941193, 0, 0.322580636, 0)
				togcmd.Size = UDim2.new(0, 306, 0, 10)
				togcmd.Font = Enum.Font.GothamMedium
				togcmd.Text = toginfo
				togcmd.TextColor3 = Color3.fromRGB(255, 255, 255)
				togcmd.TextSize = 12.000
				togcmd.TextXAlignment = Enum.TextXAlignment.Left

				togCorner.CornerRadius = UDim.new(0, 5)
				togCorner.Name = "togCorner"
				togCorner.Parent = cmdtog

				Toogle.Name = "Toogle"
				Toogle.Parent = cmdtog
				Toogle.BackgroundColor3 = Color3.fromRGB(36, 36, 36)
				Toogle.Position = UDim2.new(0.935294151, 0, 0.199999988, 0)
				Toogle.Size = UDim2.new(0, 15, 0, 15)

				togCorner_2.CornerRadius = UDim.new(0, 4)
				togCorner_2.Name = "togCorner"
				togCorner_2.Parent = Toogle
				
				togStroke.Name = "togSroke"
				togStroke.Parent = Toogle
				togStroke.Color = Color3.fromRGB(52, 52, 52)
				togStroke.Thickness = 1
				togStroke.Transparency = 0
				togStroke.Enabled = true
			local tog = false

			cmdtog.MouseButton1Click:Connect(function()
				tog = not tog
				callback(tog)
				if tog then 
					game.TweenService:Create(Toogle, TweenInfo.new(0.15, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(74, 128, 255)
					}):Play()
				else
					game.TweenService:Create(Toogle, TweenInfo.new(0.15, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(36, 36, 36)
					}):Play()
				end
			end)
		end
		
		return Elements
	end
	return Tabs
end
return coolboy
